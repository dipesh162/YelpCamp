<% include ./partials/header %>


<h1 style="font-size: 40px; text-align: center;">Login!</h1>

<div style="width: 40%; margin: 0 auto;"> 
	<form action="/login" method="POST" class="form-horizontal">
			<div class="form-group">
				<div class="col-xs-12">
				<input type="text" class="form-control" placeholder="Username" id="first_name" name="username">
				</div>
			</div>
			<div class="form-group">
				<div class="col-xs-12">
				<input type="password" class="form-control" placeholder="Password" id="last_name" name="password">
				</div>
			</div>
			<div class="form-group">
				<div class="col-xs-12">
				<input type="submit" value="Login" class="form-control">
				</div>
			</div>	
	  <!-- <input type="text" name="username" placeholder="username">
	  <input type="password" name="password" placeholder="password">
	  <input type="submit" value="login"> -->
	</form>
</div>

<% include ./partials/footer %>